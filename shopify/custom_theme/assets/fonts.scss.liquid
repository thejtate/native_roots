  url({{ 'font_din-cond-regular-webfont.woff2' | asset_url }}) format('woff2'),
  url({{ 'font_din-cond-regular-webfont.ttf' | asset_url }}) format('truetype'),
  url({{ 'font_din-cond-regular-webfont.svg#din-cond-regular' | asset_url }}) format('svg');
  font-weight: normal;
  font-style: normal;
}

